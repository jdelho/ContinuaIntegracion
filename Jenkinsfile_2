
import java.text.SimpleDateFormat;
import java.text.DateFormat;

pipeline {
    agent any 
    
    stages {
        stage("Model Examen 1") {
            steps {
                script {
                    DateFormat dateFormat = new SimpleDateFormat("HH:mm");
                    def hora = dateFormat.format(new Date())
                    println(hora)
                }
            }
        }
    }
}
